<h3>{{viewTitle}}</h3>

<form action="/feeds" method="POST" autocomplete="off">
    <input type="hidden" name="_id" value="{{feeds._id}}">
    <div class="form-group">
        <label>FEED</label>
        <input type="text" class="form-control" name="feed" placeholder="Enter Feed" value="{{feeds.feed}}">
      
    </div>
     <div class="form-group">
        <label>FEED URL</label>
        <input type="text" class="form-control" name="feed_url" placeholder="Enter Feed Url" value="{{feeds.feed_url}}">
      
    </div>
    <div class="form-group">
        <button type="submit" class="btn btn-info"><i class="fa fa-database"></i> Submit</button>
        <a class="btn btn-secondary" href="/feeds/list"><i class="fa fa-list-alt"></i> View All</a>
    </div>
</form>